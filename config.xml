<?xml version='1.0' encoding='utf-8'?>
<widget id="com.leediancn.fofadon" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>凤妃堂</name>
    <icon density="mdpi" height="96" platform="android" src="res/icon/fonton/fonlogo.png" width="96" />
    <description>
      凤妃堂
    </description>
    <author email="cain.chu@leediancn.com" href="http://leediancn.com">
      凤妃堂
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="orientation" value="portrait" />
    <preference name="QQ_APP_ID" value="101451885" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-qqsdk" spec="^0.9.7">
        <variable name="QQ_APP_ID" value="101451885" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
</widget>
